{
  "name": "laravel",
  "scripts": {
    "postdeploy": "bin/setup"
  },
  "env": {
    "APP_DEBUG": {
      "required": true
    },
    "APP_ENV": {
      "required": true
    },
    "APP_KEY": {
      "required": true
    },
    "APP_LOG_LEVEL": {
      "required": true
    },
    "APP_NAME": {
      "required": true
    },
    "APP_URL": {
      "required": true
    },
    "CLEARDB_DATABASE_URL": {
      "required": true
    },
    "GITHUB_USERNAME": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": [
    "cleardb"
  ],
  "buildpacks": [
    {
      "url": "heroku/php"
    }
  ]
}
